{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1",
      "status": 200
    }
  ],
  "env": {
    "REACT_APP_SUPABASE_URL": "your_supabase_project_url",
    "REACT_APP_SUPABASE_ANON_KEY": "your_supabase_anon_key"
  }
}